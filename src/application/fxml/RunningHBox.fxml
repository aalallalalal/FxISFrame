<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="730.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="project_name" prefHeight="20.0" prefWidth="200.0" text="Label">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Label>
      <VBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="200.0">
         <children>
            <ProgressBar fx:id="progress" maxHeight="23.0" prefHeight="23.0" prefWidth="195.0" progress="0.0" />
            <Label fx:id="currentState" prefHeight="20.0" prefWidth="100.0" text="Label">
               <VBox.margin>
                  <Insets left="3.0" top="3.0" />
               </VBox.margin></Label>
         </children>
      </VBox>
      <JFXButton fx:id="paramDetail" text="修改参数">
         <HBox.margin>
            <Insets left="40.0" />
         </HBox.margin></JFXButton>
      <JFXButton fx:id="restart" text="重新运行">
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin></JFXButton>
      <JFXButton text="关闭">
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
      </JFXButton>
   </children>
</HBox>
