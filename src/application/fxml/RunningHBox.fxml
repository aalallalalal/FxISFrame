<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="730.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="40.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
         <image>
            <Image url="@../../../bin/resources/renwu.png" />
         </image>
      </ImageView>
      <VBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="150.0">
         <children>
            <Label fx:id="project_name" prefHeight="20.0" prefWidth="150.0" >
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="runningInfo" prefHeight="18.0" prefWidth="440.0" >
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
         </children>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </VBox>
      <ProgressIndicator fx:id="run" prefHeight="16.0" prefWidth="20.0" visible="false">
         <HBox.margin>
            <Insets left="50.0" />
         </HBox.margin>
      </ProgressIndicator>
      <JFXButton fx:id="paramDetail" buttonType="RAISED" text="%ParamDetail">
         <HBox.margin>
            <Insets left="260.0" />
         </HBox.margin></JFXButton>
      <JFXButton fx:id="close" text="%Shut">
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
      </JFXButton>
   </children>
</HBox>
