<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>

<BorderPane style="-fx-background-color:#fff" fx:id="root"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	fx:controller="application.control.SettingController">
	<center>
		<VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0"
			style="-fx-padding: 10;" BorderPane.alignment="CENTER">
			<children>
				<JFXCheckBox fx:id="checkBox_SaveMiddle" text="保存中间中间结果" selected="false">
					<styleClass>
						<String fx:value="custom-jfx-check-box" />
					</styleClass>
				</JFXCheckBox>
				<HBox prefHeight="100.0" prefWidth="200.0" spacing="6.0">
					<children>
						<Label text="图像划分网格大小:">
							<styleClass>
								<String fx:value="label-leve-normal" />
							</styleClass>
						</Label>
						<VBox prefHeight="200.0" prefWidth="100.0">
							<children>
								<HBox alignment="CENTER_LEFT" prefHeight="60.0"
									prefWidth="200.0">
									<children>
										<Label text="宽(像素):">
											<styleClass>
												<String fx:value="label-leve-normal" />
											</styleClass>
										</Label>
										<JFXTextArea fx:id="textArea_width">
											<styleClass>
												<String fx:value="custom-text-area" />
											</styleClass>
										</JFXTextArea>
									</children>
								</HBox>
								<HBox alignment="CENTER_LEFT" prefHeight="60.0"
									prefWidth="200.0">
									<children>
										<Label text="高(像素):">
											<styleClass>
												<String fx:value="label-leve-normal" />
											</styleClass>
										</Label>
										<JFXTextArea fx:id="textArea_hight">
											<styleClass>
												<String fx:value="custom-text-area" />
											</styleClass>
										</JFXTextArea>
									</children>
								</HBox>
							</children>
						</VBox>
					</children>
				</HBox>
				<JFXCheckBox text="进行重叠率先验约束" fx:id="checkBox_preCheck"
					selected="false">
					<styleClass>
						<String fx:value="custom-jfx-check-box" />
					</styleClass>
				</JFXCheckBox>
				<HBox alignment="CENTER_LEFT" prefHeight="60.0" disable="true"
					fx:id="hbox_preCheckDetail" prefWidth="200.0" spacing="6.0">
					<children>
						<Label text="无人机飞行高度(米):">
							<styleClass>
								<String fx:value="label-leve-normal" />
							</styleClass>
						</Label>
						<JFXTextArea fx:id="textArea_flyHeight">
							<styleClass>
								<String fx:value="custom-text-area" />
							</styleClass>
						</JFXTextArea>
						<Label text="相机像元大小(微米):">
							<styleClass>
								<String fx:value="label-leve-normal" />
							</styleClass>
						</Label>
						<JFXTextArea fx:id="textArea_cameraSize">
							<styleClass>
								<String fx:value="custom-text-area" />
							</styleClass>
						</JFXTextArea>
					</children>
				</HBox>
			</children>
			<BorderPane.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</BorderPane.margin>
		</VBox>
	</center>
</BorderPane>
